<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_case_file" model="ir.sequence">
            <field name="name">Case File</field>
            <field name="code">case.file</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <!-- <field eval="1" name="number_next"/>			 -->
            <field eval="1" name="number_increment"/>			
            <field eval="True" name="use_date_range"/>			
            <field name="padding">4</field>	
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_case_contact" model="ir.sequence">
            <field name="name">Case File Contact</field>
            <field name="code">case.contact</field>
            <field name="prefix">CS</field>
            <field name="padding">8</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_case_file_category" model="ir.sequence">
            <field name="name">Case File Category</field>
            <field name="code">case.file.category</field>
            <field name="prefix">CC</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_case_file_subcategory" model="ir.sequence">
            <field name="name">Case File Subcategory</field>
            <field name="code">case.file.subcategory</field>
            <field name="prefix">CS</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_court" model="ir.sequence">
            <field name="name">Court</field>
            <field name="code">court</field>
            <field name="prefix">COURT-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>

        <record id="seq_case_document" model="ir.sequence">
            <field name="name">Case Document</field>
            <field name="code">case.document</field>
            <field name="prefix">CDOC-</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_case_act" model="ir.sequence">
            <field name="name">Case Act</field>
            <field name="code">case.act</field>
            <field name="prefix">CACT-</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
        <record id="seq_case_reference" model="ir.sequence">
            <field name="name">Case Reference</field>
            <field name="code">case.reference</field>
            <field name="prefix">CREF-</field>
            <field name="padding">8</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="active">TRUE</field>
        </record>
    </data>
</odoo>
